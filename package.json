{
  "name": "jigsaw-puzzle",
  "version": "1.0.0",
  "description": "A plain jigsaw puzzle implemented in Elm",
  "license": "MIT",
  "scripts": {
    "start": "run-p --print-label dev serve",
    "dev": "nodemon --ext elm,html --watch ./src --watch ./public --exec './build.sh'",
    "serve": "http-server -c-1 ./dist",
    "build": "PROD=true ./build.sh",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/autologie/jigsaw-puzzle.git"
  },
  "homepage": "https://github.com/autologie/jigsaw-puzzle/blob/master/README.md",
  "devDependencies": {
    "elm": "^0.19.0-bugfix2",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "elm-test": "^0.19.0-rev3",
    "uglify-js": "^3.4.9"
  }
}
